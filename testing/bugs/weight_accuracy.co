network N;

double gewicht = 0.001;

int i = N.addNode(parameterGrowth());
N.setParam(i, "parameterGrowth_w", gewicht);
N.observe(i, "output_a", component(0));
N.evolve(0.0,0.02);

N.clear();
N.removeObserver();

i = N.completeNetwork(2, weightSumGrowth(), weightedEdge(gewicht));
N.observe(i, "output_b", component(0));
N.evolve(0.0,0.02);
